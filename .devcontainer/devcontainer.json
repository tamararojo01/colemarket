{
  "name": "Colemarket â€¢ Flutter Web",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "postCreateCommand": "bash .devcontainer/setup.sh && flutter --version && flutter precache --web && flutter config --enable-web",
  "forwardPorts": [5173],
  "portsAttributes": {
    "5173": { "label": "Flutter Web", "onAutoForward": "openBrowser" }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter",
        "EditorConfig.EditorConfig"
      ],
      "settings": {
        "dart.lineLength": 100,
        "files.eol": "\n",
        "terminal.integrated.defaultProfile.linux": "bash"
      }
    }
  }
}
